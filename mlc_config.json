{
  "aliveStatusCodes": [
    403,
    200
  ],
  "httpHeaders": [
    {
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      },
      "urls": [
        "https://github.com/",
        "https://guides.github.com/",
        "https://help.github.com/",
        "https://docs.github.com/"
      ]
    },
    {
      "headers": {
        "Accept": "application/vnd.github+json, application/vnd.github.object",
        "Authorization": "token TOKEN"
      },
      "urls": [
        "https://api.github.com",
        "https://raw.githubusercontent.com"
      ]
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "localhost"
    },
    {
      "pattern": "github.com/department-of-veterans-affairs/(?=.+/settings/.+)"
    },
    {
      "pattern": "github.com(?=.+/pkgs)"
    },
    {
      "pattern": "github.com(?=.+/actions$)"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "https://github.com/((?!orgs|repos)[a-zA-Z0-9_-]+)(?!.+/blob/.+)",
      "replacement": "https://api.github.com/repos/$1"
    },
    {
      "pattern": "(?<=department-of-veterans-affairs)([/]+[a-zA-Z0-9-_.]+)[/]?$",
      "replacement": "$1/contents/"
    },
    {
      "pattern": "github.com/department-of-veterans-affairs/(?=.+/blob/.+)",
      "replacement": "raw.githubusercontent.com/department-of-veterans-affairs/"
    },
    {
      "pattern": "github.com/([a-zA-Z0-9-_.]+)(?=.+/blob/.+)",
      "replacement": "raw.githubusercontent.com/$1"
    },
    {
      "pattern": "tree/([a-zA-Z0-9-_.]+)$",
      "replacement": "branches/$1"
    },
    {
      "pattern": "tree/(main|master)/",
      "replacement": "contents/"
    },
    {
      "pattern": "pull/([0-9]+)/files$",
      "replacement": "pulls/$1"
    },
    {
      "pattern": "pull/([0-9]+)",
      "replacement": "pulls/$1"
    },
    {
      "pattern": "/blob",
      "replacement": ""
    }
  ]
}
